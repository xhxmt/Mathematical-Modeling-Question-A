    "        missiles[f'M{i+1}'] = {\n",
    "            'initial_pos': np.array([20000.0 + i*100, i*50, 2000.0]),\n",
    "            'target_pos': np.array([0.0, 200.0, 5.0]),\n",
    "            'speed': 300.0\n",
    "        }\n",
    "    \n",
    "    # 生成多个目标点\n",
    "    target_points = []\n",
    "    for i in range(target_count):\n",
    "        angle = 2 * np.pi * i / target_count\n",
    "        radius = 100\n",
    "        target_points.append(np.array([\n",
    "            radius * np.cos(angle), \n",
    "            200 + radius * np.sin(angle), \n",
    "            5.0\n",
    "        ]))\n",
    "    \n",
    "    return {\n",
    "        'missiles': missiles,\n",
    "        'drones': {\n",
    "            'FY1': {\n",
    "                'initial_pos': np.array([17800.0, 0.0, 1800.0]),\n",
    "                'velocity': np.array([-120.0, 0.0, 0.0]),\n",
    "                'grenades': [{'grenade_id': 1, 'drop_time': 1.5, 'detonation_delay': 3.6}]\n",
    "            }\n",
    "        },\n",
    "        'target_points': target_points\n",
    "    }\n",
