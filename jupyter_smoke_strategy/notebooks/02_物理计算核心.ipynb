    "    \n",
    "    # 分析线性关系\n",
    "    df = pd.DataFrame(results)\n",
    "    \n",
    "    # 计算相关系数\n",
    "    correlation_time = np.corrcoef(df['time_steps'], df['calc_time'])[0,1]\n",
    "    correlation_missiles = np.corrcoef(df['missiles'], df['calc_time'])[0,1]\n",
    "    correlation_targets = np.corrcoef(df['targets'], df['calc_time'])[0,1]\n",
    "    correlation_ops = np.corrcoef(df['theoretical_ops'], df['calc_time'])[0,1]\n",
    "    \n",
    "    print(f\"\\n📊 线性相关性分析:\")\n",
    "    print(f\"  时间步数 - 计算时间: {correlation_time:.3f}\")\n",
    "    print(f\"  导弹数量 - 计算时间: {correlation_missiles:.3f}\")\n",
    "    print(f\"  目标数量 - 计算时间: {correlation_targets:.3f}\")\n",
    "    print(f\"  理论操作数 - 实际时间: {correlation_ops:.3f}\")\n",
    "    \n",
    "    return df\n",
    "\n",
    "def create_test_scenario(missile_count, target_count):\n",
    "    \"\"\"创建指定规模的测试场景\"\"\"\n",
    "    # 生成多个导弹\n",
