    "# 传统循环实现（用于性能对比）\n",
    "def missile_trajectory_loop(initial_pos, target_pos, speed, time_array):\n",
    "    \"\"\"传统循环实现的导弹轨迹计算\"\"\"\n",
    "    direction = target_pos - initial_pos\n",
    "    direction_norm = np.linalg.norm(direction)\n",
    "    if direction_norm < 1e-10:\n",
    "        return np.tile(initial_pos, (len(time_array), 1))\n",
    "    \n",
    "    unit_direction = direction / direction_norm\n",
    "    velocity = unit_direction * speed\n",
    "    \n",
    "    # 循环计算每个时间点\n",
    "    positions = np.zeros((len(time_array), 3))\n",
    "    for i, t in enumerate(time_array):\n",
    "        positions[i] = initial_pos + velocity * t\n",
    "    \n",
    "    return positions\n",
    "\n",
    "# 性能测试\n",
    "print(\"⚡ 轨迹计算性能对比测试\")\n",
    "print(\"=\" * 60)\n",
    "\n",
    "# 测试参数\n",
    "missile_start = np.array([20000.0, 0.0, 2000.0])\n",
    "missile_target = np.array([0.0, 200.0, 5.0])\n",
    "missile_speed = 300.0\n",
    "\n",
    "# 循环方法测试\n",
    "start_time = time.perf_counter()\n",
    "result_loop = missile_trajectory_loop(missile_start, missile_target, missile_speed, time_fine)\n",
    "time_loop = time.perf_counter() - start_time\n",
    "\n",
    "# 向量化方法测试\n",
    "start_time = time.perf_counter()\n",
    "result_vectorized = EnhancedTrajectoryCalculator.missile_trajectory_array(\n",
    "    missile_start, missile_target, missile_speed, time_fine)\n",
    "time_vectorized = time.perf_counter() - start_time\n",
    "\n",
    "# 结果验证\n",
    "max_error = np.max(np.abs(result_loop - result_vectorized))\n",
    "speedup = time_loop / time_vectorized\n",
    "\n",
    "print(f\"循环方法用时:     {time_loop*1000:.3f} ms\")\n",
    "print(f\"向量化方法用时:   {time_vectorized*1000:.3f} ms\")\n",
    "print(f\"加速比:          {speedup:.1f}x\")\n",
    "print(f\"最大误差:        {max_error:.2e} 米\")\n",
    "print(f\"数据点数:        {len(time_fine):,} 个\")\n",
    "\n",
    "print(f\"\\n✅ 向量化实现比循环快 {speedup:.1f} 倍，误差 < {max_error:.1e}\")"
   ]
