    "# 复杂度分析实验\n",
    "print(\"📈 算法复杂度理论验证\")\n",
    "print(\"=\" * 60)\n",
    "\n",
    "def complexity_analysis():\n",
    "    \"\"\"分析不同参数对计算复杂度的影响\"\"\"\n",
    "    \n",
    "    # 测试维度\n",
    "    time_steps = [100, 200, 500, 1000, 2000]\n",
    "    missile_counts = [1, 2, 5, 10]\n",
    "    target_counts = [1, 4, 8, 16]\n",
    "    \n",
    "    results = []\n",
    "    \n",
    "    print(\"🔍 多维复杂度分析:\")\n",
    "    \n",
    "    for t_steps in [500, 1000]:  # 选择代表性时间步数\n",
    "        for m_count in missile_counts:\n",
    "            for target_count in target_counts:\n",
    "                # 创建测试场景\n",
    "                scenario = create_test_scenario(m_count, target_count)\n",
    "                \n",
    "                # 计时测试\n",
    "                sim_engine = InteractiveSimulationEngine(time_step=40.0/t_steps)\n",
    "                start_time = time.perf_counter()\n",
    "                _ = sim_engine.simulate_scenario(scenario, max_time=40.0)\n",
    "                calc_time = time.perf_counter() - start_time\n",
    "                \n",
    "                result = {\n",
    "                    'time_steps': t_steps,\n",
    "                    'missiles': m_count,\n",
    "                    'targets': target_count,\n",
    "                    'calc_time': calc_time,\n",
    "                    'theoretical_ops': t_steps * m_count * target_count\n",
    "                }\n",
    "                results.append(result)\n",
    "                \n",
    "                if m_count <= 2 and target_count <= 4:  # 只显示部分结果\n",
